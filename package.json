    "node-static": "^0.7.4",
    "observ": "^0.2.0",
    "observ-struct": "^6.0.0",
    "value-event": "^5.0.0",
    "virtual-dom": "^2.1.1",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "@course-search/get-credit-count": "file:get-credit-count",
    "browserify": "^12.0.1",
    "bundle-collapser": "^1.1.1",
    "cheerio": "^0.19.0",
    "concat-stream": "^1.4.6",
    "graceful-fs": "^4.1.2",
    "hyperquest": "^1.0.1",
    "nodemon": "^1.2.1",
    "pretty-bytes-cli": "^1.0.0",
    "uglify-js": "^2.4.15",
    "uglifyify": "^3.0.1",
